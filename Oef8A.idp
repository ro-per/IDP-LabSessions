vocabulary V {
    type Dag
    Kerstmis: Dag
    Nieuwjaar : Dag
}

structure S: V {
    Dag = { Maandag; Dinsdag; Woensdag; Donderdag ;Vrijdag; Zaterdag ; Zondag }
    
    Kerstmis = Maandag
}

theory T: V{
    Dag(Nieuwjaar).
}


procedure main() {
    stdoptions .nbmodels = 7
    printmodels(modelexpand(T, S))
}